<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="yodoo_knowledge_item_view_form" model="ir.ui.view">
        <field name="model">yodoo.knowledge.item</field>
        <field name="inherit_id" ref="yodoo_knowledge.yodoo_knowledge_item_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='category_id']" position="after">
                <field name="business_domain_id" widget="many2one" readonly="1"/>
            </xpath>
        </field>
    </record>
</odoo>
